/include/ "system-conf.dtsi"
/ {
	osc: clkc {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <20000000>;
	};
};

/*&gpio0 {
	#interrupt-cells = <2>;
	interrupt-controller;
	// #reset-cells = <1>;
};*/

&spi1 {
	is-decoded-cs = <0>;
	num-cs = <1>;
	status = "okay";
	can0 {
		compatible = "microchip,mcp25625";
		spi-max-frequency = <10000000>;
		interrupt-parent = <&intc>;
		interrupts = <0 29 4>;
		clocks = <&osc>;
		reg = <0>;
		//reset-gpios = <&gpio0 9 1>;
	        //#reset-cells = <0>;
		//resets = <&gpio0 9>;
	};
};

