/include/ "system-conf.dtsi"
/ {
	osc: clkc {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <10000000>;
	};
};

&gpio0 {
	#interrupt-cells = <2>;
	interrupt-controller;
	// #reset-cells = <1>;
};

&spi1 {
	is-decoded-cs = <0>;
	num-cs = <1>;
	status = "okay";
	can2 {
		compatible = "microchip,mcp25625";
		spi-max-frequency = <10000000>;
		clocks = <&osc>;
		interrupt-parent = <&gpio0>;
		interrupts = <0 0x2>;
		reg = <0>;
	        #reset-cells = <0>;
		resets = <&gpio0 9>;
	};
};

